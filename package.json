{
  "name": "graphql-apollo-poc",
  "version": "1.0.0",
  "description": "A poc for graphql Apollo",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "concurrently --kill-others --names \"server,compiler\" \"yarn gen:types\" \"styleguidist server\"",
    "build": "styleguidist build",
    "devserver": "webpack serve --progress --hot --config webpack.config.js",
    "lint": "eslint --ext .ts,.tsx,.js src",
    "lint:quiet": "eslint --quiet --ext .ts,.tsx,.js src",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js src",
    "download:schema": "npx apollo-codegen introspect-schema http://localhost:4000/graphql --header --output schema.json",
    "generate-json-from-local-graphql": "npx apollo-codegen introspect-schema schema.graphql --output schema.json",
    "generate:types": "npx apollo-codegen generate **/*.graphql --schema schema.json --target typescript --output src/gql.ts",
    "gen:types": "graphql-codegen --config codegen.yaml",
    "prepare": "npx husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fangfang-POC/graphql-apollo-poc.git"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "graphql": "^16.3.0",
    "graphql-ws": "^5.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "lodash.isempty": "^4.4.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-react-apollo": "^3.2.11",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/lodash.isempty": "^4.4.6",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@webpack-cli/serve": "^1.6.1",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "concurrently": "^4.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.3.2",
    "react-styleguidist": "^11.2.0",
    "react-docgen-typescript": "^2.2.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",   
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "author": "FangfangHao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fangfang-POC/graphql-apollo-poc/issues"
  },
  "homepage": "https://github.com/Fangfang-POC/graphql-apollo-poc#readme"
}
